[Unit]
Description=JNA Wedding Instance
After=docker.service
Requires=docker.service

[Service]
TimeoutStartSec=0
Restart=always
ExecStartPre=-/usr/local/bin/docker-compose -f /var/www/repos/wedding/docker-compose.yml down
ExecStart=/usr/local/bin/docker-compose -f /var/www/repos/wedding/docker-compose.yml up

[Install]
WantedBy=multi-user.target
